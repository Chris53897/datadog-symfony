{
    "name":"okvpn/datadog-symfony",
    "type":"symfony-bundle",
    "license":"MIT",
    "description": "Symfony Datadog integration",
    "keywords": ["DogStatsD", "datadog-symfony", "php-datadog"],
    "authors": [
        {
            "name": "Uladzimir Tsykun",
            "homepage": "https://github.com/vtsykun",
            "email": "vtsykun@okvpn.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Okvpn\\Bundle\\DatadogBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "Okvpn\\Bundle\\DatadogBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php":">=8.0",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0",
        "graze/dog-statsd": "^1.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.13",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "4.4 || ^5.0 || ^6.0",
        "symfony/console": "^4.4 || ^5.0 || ^6.0",
        "symfony/security-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.0 || ^6.0",
        "symfony/var-dumper": "^4.4 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^3.8",
        "phpstan/phpstan": "^1.8"
    },
    "scripts": {
        "phpstan": "phpstan analyse src --no-progress --level=6"
    }
}
